<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.*?>
<AnchorPane prefHeight="1600.0" prefWidth="1920.0" styleClass="mainMenu" stylesheets="@Styles.css"
            xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="view.Graphic.StartNewGame">
    <ImageView fitHeight="100.0" fitWidth="80.0" onMouseClicked="#exit" AnchorPane.leftAnchor="0.0"
               AnchorPane.topAnchor="0.0">
        <Image url="@images/backButton.png">
        </Image>
    </ImageView>
    <VBox spacing="5" styleClass="menuText" AnchorPane.leftAnchor="200" AnchorPane.topAnchor="400">
        <Label style="-fx-text-fill: white" text="Single Player" onMouseClicked="#goToSinglePlayerMode"
               onMouseEntered="#onMenuItem" onMouseExited="#outOfMenuItem"/>
        <Label style="-fx-text-fill: white" text="Multi Player" onMouseClicked="#goToMultiPlayerMode"
               onMouseEntered="#onMenuItem" onMouseExited="#outOfMenuItem"/>
    </VBox>
</AnchorPane>
